{
  "name": "@GerHobbelt/live-server",
  "version": "1.3.0-17",
  "description": "simple development http server with live reload capability",
  "keywords": [
    "front-end",
    "development",
    "tool",
    "server",
    "http",
    "cli"
  ],
  "author": "Tapio Vierros",
  "dependencies": {
    "chokidar": "1.6.1",
    "colors": "1.1.2",
    "connect": "3.6.1",
    "cors": "2.8.3",
    "event-stream": "3.3.4",
    "faye-websocket": "0.11.1",
    "formidable": "1.1.1",
    "http-auth": "3.1.3",
    "marked": "0.3.6",
    "mkdirp": "0.5.1",
    "morgan": "1.8.1",
    "object-assign": "4.1.1",
    "opn": "5.0.0",
    "proxy-middleware": "0.15.0",
    "send": "0.15.2",
    "serve-index": "1.8.0",
    "stream-sink": "GerHobbelt/node-stream-sink#master",
    "spdy": "3.4.4"
  },
  "devDependencies": {
    "mocha": "3.3.0",
    "supertest": "3.0.0",
    "eslint": "3.19.0",
    "jshint": "2.9.4"
  },
  "scripts": {
    "lint": "eslint live-server.js index.js",
    "hint": "jshint live-server.js index.js",
    "test": "mocha test && npm run lint && npm run hint"
  },
  "bin": {
    "live-server": "./live-server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GerHobbelt/live-server.git"
  },
  "engines": {
    "node": ">=4.0"
  },
  "preferGlobal": true,
  "license": "MIT",
  "eslintConfig": {
    "env": {
      "node": true
    },
    "rules": {
      "quotes": 0,
      "curly": 0,
      "strict": 0,
      "no-process-exit": 0,
      "eqeqeq": 1,
      "no-unused-vars": 1,
      "no-shadow": 1
    }
  }
}
